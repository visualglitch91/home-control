{
  "name": "@home-control/client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prerun": "echo \"\\\"$(date +'%Y.%m.%d-%H:%M:%S')\\\"\" > src/version.json",
    "dev": "npm run prerun && vite",
    "build": "npm run prerun && tsc && vite build",
    "postbuild": "cp src/robots.txt ../../dist && cp -R src/assets/icons ../../dist/icons && cp src/version.json ../../dist/latest.json"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/joy": "^5.0.0-beta.7",
    "@tanstack/query-sync-storage-persister": "^4.35.3",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-persist-client": "^4.35.5",
    "@uiw/react-color-wheel": "^1.4.2",
    "axios": "^1.5.0",
    "color-rgba": "^3.0.0",
    "home-assistant-js-websocket": "^8.2.0",
    "humanize-duration": "^3.30.0",
    "macy": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "shell-color": "^2.0.1",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@types/humanize-duration": "^3.27.1",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.6",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.16.5"
  }
}
